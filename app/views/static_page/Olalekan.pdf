<script type="text/javascript">
    $(function() {                   

    $('[id^="bk"]').click(function(e) {
        history.go(-1);
        e.preventDefault();
    })
    
slip_printed = false;

$(document).ready(function(){
	var group_btn = $(".btn-group").children().removeClass("active");	 
	var submit_btn = group_btn[2];
	$(submit_btn).addClass("active");
	$(".print_btn").click(function(){ 
		var content = $(".preview").clone();
		$("#print_slip").contents().find(".span12").html(content);
		
		//slip_printed = true;		
		window.frames["print_slip"].focus();
		window.frames["print_slip"].print();	 
	});
});  


$(window).on('beforeunload', function(ev) {
    var message = "Please print your confirmation slip before leaving this page. Once you leave this page you will not be able to print your confirmation slip again!"

    if (slip_printed === false) {
        ev = ev || window.event;

        // For IE and Firefox prior to version 4
        if (ev) {
            ev.returnValue = message;
        }


        return message;
    }
    
});


    })

</script>

